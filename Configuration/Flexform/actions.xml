        <s_showContentElements>
            <ROOT>
            <TCEforms>
                <sheetTitle>LLL:EXT:mkabtesting/Resources/Private/Language/flexform.xml:plugin.mkabtesting.flexform.action.ShowContentElements</sheetTitle>
            </TCEforms>
            <type>array</type>
            <el>
                <showContentElements.elements>
                    <TCEforms>
                        <exclude>1</exclude>
                        <label>LLL:EXT:mkabtesting/Resources/Private/Language/flexform.xml:plugin.mkabtesting.showContentElements.elements</label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>tt_content</allowed>
                            <size>5</size>
                            <minitems>2</minitems>
                            <maxitems>999</maxitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </TCEforms>
                </showContentElements.elements>
                <showContentElements.numberOfElementsToDisplay>
                    <TCEforms>
                        <exclude>1</exclude>
                        <label>LLL:EXT:mkabtesting/Resources/Private/Language/flexform.xml:plugin.mkabtesting.showContentElements.numberOfElementsToDisplay</label>
                        <config>
                            <type>input</type>
                            <default>1</default>
                            <eval>trim,required,int</eval>
                            <range>
                                <lower>1</lower>
                                <upper>999</upper>
                            </range>
                        </config>
                    </TCEforms>
                </showContentElements.numberOfElementsToDisplay>
                <showContentElements.campaignIdentifier>
                    <TCEforms>
                        <exclude>1</exclude>
                        <label>LLL:EXT:mkabtesting/Resources/Private/Language/flexform.xml:plugin.mkabtesting.showContentElements.campaignIdentifier</label>
                        <config>
                            <type>input</type>
                            <eval>trim,required</eval>
                            <maxsize>255</maxsize>
                        </config>
                    </TCEforms>
                </showContentElements.campaignIdentifier>
                <showContentElements.cookieExpireTime>
                    <TCEforms>
                        <exclude>1</exclude>
                        <label>LLL:EXT:mkabtesting/Resources/Private/Language/flexform.xml:plugin.mkabtesting.showContentElements.cookieExpireTime</label>
                        <config>
                            <type>input</type>
                            <default>2592000</default>
                            <eval>trim,required,int</eval>
                        </config>
                    </TCEforms>
                </showContentElements.cookieExpireTime>
            </el>
            </ROOT>
        </s_showContentElements>
